{
  "$schema": "http://json.schemastore.org/template",
  "author": "BepInEx",
  "classifications": [ "BepInEx", "BepInEx 6" , "Plugin", "Unity", "Il2Cpp" ],
  "identity": "BepInEx6.Plugin.Unity.Il2Cpp",
  "name": "BepInEx 6 Unity Il2Cpp Plugin",
  "shortName": "bep6plugin_unity_il2cpp",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "BepInEx6.PluginTemplate.Unity.Il2Cpp",
  "symbols": {
    "TargetFramework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "text",
      "defaultValue": "netstandard2.1",
      "replaces": "netstandard2.1"
    },
    "Description": {
      "type": "parameter",
      "description": "Plugin description",
      "datatype": "text",
      "replaces": "My first plugin",
      "defaultValue": "My first plugin"
    },
    "Version": {
      "type": "parameter",
      "description": "Plugin version",
      "datatype": "text",
      "defaultValue": "1.0.0"
    },
    "VersionImpl": {
      "type": "generated",
      "generator": "join",
      "replaces": "<Version>1.0.0</Version>",
      "parameters": {
        "symbols": [
          {
            "type": "const",
            "value": "<Version>"
          },
          {
            "type": "ref",
            "value": "Version"
          },
          {
            "type": "const",
            "value": "</Version>"
          }
        ]
      }
    }
  }
}