{
  "$schema": "http://json.schemastore.org/template",
  "author": "BepInEx",
  "classifications": [ "BepInEx", "BepInEx 6" , "Plugin" , "Unity", "Mono" ],
  "identity": "BepInEx6.PluginTemplate.Unity.Mono",
  "name": "BepInEx 6 Unity Mono Plugin",
  "shortName": "bep6plugin_unity_mono",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "preferNameDirectory": true,
  "sourceName": "BepInEx6.PluginTemplate.Unity.Mono",
  "symbols": {
    "TargetFramework": {
      "type": "parameter",
      "description": "The target framework for the project",
      "datatype": "text",
      "defaultValue": "net35",
      "replaces": "net35"
    },
    "Description": {
      "type": "parameter",
      "description": "Plugin description",
      "datatype": "text",
      "replaces": "My first plugin",
      "defaultValue": "My first plugin"
    },
    "UnityVersion": {
      "type": "parameter",
      "description": "Unity version to use when developing the plugin",
      "datatype": "text",
      "replaces": "5.6.0",
      "defaultValue": "5.6.0"
    },
    "Version": {
      "type": "parameter",
      "description": "Plugin version",
      "datatype": "text",
      "replaces": "{version}",
      "defaultValue": "1.0.0"
    }
  }
}